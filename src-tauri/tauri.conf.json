{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "dskmgr",
	"version": "0.1.0",
	"identifier": "com.dskmgr.desktop",
	"build": {
		"frontendDist": "../build-static",
		"devUrl": "http://localhost:5173",
		"beforeDevCommand": "npm run start:server-only",
		"beforeBuildCommand": "npm run build:static"
	},
	"app": {
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"macOS": {
			"signingIdentity": "-"
		}
	}
}
